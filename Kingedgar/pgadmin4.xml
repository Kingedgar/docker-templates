<?xml version="1.0"?>
<Container version="2">
  <Name>pgAdmin4</Name>
  <Repository>kingedgar/pgadmin4</Repository>
  <Registry>https://hub.docker.com/r/kingedgar/pgadmin4/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support></Support>
  <Project/>
  <Overview>pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL, the most advanced Open Source database in the world</Overview>
  <Category>Tools:Utilities</Category>
  <WebUI>http://[IP]:[PORT:5050]/</WebUI>
  <TemplateURL>https://github.com/Kingedgar/docker-templates/blob/master/Kingedgar/pgadmin4.xml</TemplateURL>
  <Icon>http://i.imgur.com/N4wDUnJ.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>pgAdmin is the most popular and feature rich Open Source administration and development platform for PostgreSQL, the most advanced Open Source database in the world</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5050</HostPort>
        <ContainerPort>5050</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/docker-containers/pgadmin/config/</HostDir>
      <ContainerDir>/var/lib/pgadmin</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Port 1" Target="5050" Default="5050" Mode="tcp" Description="The UI port" Type="Port" Display="always" Required="true" Mask="null">5050</Config>
  <Config Name="Host Path 1" Target="/var/lib/pgadmin" Default="" Mode="rw" Description="Container Path: /var/lib/pgadmin" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/docker-containers/pgadmin/config/</Config>
</Container>
